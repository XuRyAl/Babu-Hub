-- –ò—Å–ø—Ä–∞–≤–ª–µ–Ω–Ω—ã–π logLabel!
local logLabel = tabUNI.newLabel("Log: ..")
local function setLog(msg)
    if logLabel and msg then
        logLabel.Text = "Log: " .. tostring(msg):sub(1, 100)
    end
end

tabUNI.newButton("üåÄ Toggle UI", "–°–∫—Ä—ã—Ç—å/–ø–æ–∫–∞–∑–∞—Ç—å –æ–∫–Ω–æ (V)", function()
    window:Toggle()
end)

local function SafeLoadScript(url, name)
    setLog("–ó–∞–≥—Ä—É–∑–∫–∞: " .. name)
    local success, err = pcall(function()
        loadstring(game:HttpGet(url))()
    end)
    if success then
        setLog("‚úÖ " .. name .. " —É—Å–ø–µ—à–Ω–æ –∑–∞–ø—É—â–µ–Ω!")
    else
        setLog("‚ùå " .. name .. " –æ—à–∏–±–∫–∞ –∑–∞–ø—É—Å–∫–∞: " .. tostring(err))
    end
end

