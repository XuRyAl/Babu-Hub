local DrRay = loadstring(game:HttpGet("https://raw.githubusercontent.com/XuRyAl/-CopyRight-AZYsgithub-DrRay-ui-Library/refs/heads/main/DrRay%20Script"))()
local window = DrRay:Load("Babu Hub", "Cherry")

